<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- OLD SEQUENCE -->
        <record id="seq_move_order" model="ir.sequence">
            <field name="name">Move Order</field>
            <field name="code">move.order</field>
            <field name="prefix">MO/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
        </record>

        <!-- NEW SEQUENCE -->
        <record id="seq_pi" model="ir.sequence">
            <field name="name">Physical Inventory</field>
            <field name="code">physical.inventory</field>
            <field name="prefix">PI/NPN/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_iu" model="ir.sequence">
            <field name="name">Internal Use</field>
            <field name="code">internal.use</field>
            <field name="prefix">IU/NPN/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
        </record>

        <!-- <record id="seq_iu" model="ir.sequence">
            <field name="name">Internal Use</field>
            <field name="code">internal.use</field>
            <field name="prefix">AAP/%(y)s/%(month)s/</field>
            <field name="padding">5</field>
        </record> -->
    </data>
</odoo>
